#include <stdio.h>
void swap(int *a, int *b) 
{
    int temp = *a;
    *a = *b;       
    *b = temp;   
}
int main() 
{
    int num1, num2;
    int *ptr1, *ptr2; 
    printf("Enter two integers:\n");
    printf("First number: ");
    scanf("%d", &num1);
    printf("Second number: ");
    scanf("%d", &num2);
    ptr1 = &num1;
    ptr2 = &num2;
    printf("Before swapping: num1 = %d, num2 = %d\n", *ptr1, *ptr2);
    swap(ptr1, ptr2);
    printf("After swapping: num1 = %d, num2 = %d\n", *ptr1, *ptr2);
    return 0; 
}
